        <script>
            $(document).ready(function () {
                $("#owl-demo").owlCarousel({

                    autoPlay: 3000, //Set AutoPlay to 3 seconds

                    items: 5,
                    itemsDesktop: [640, 4],
                    itemsDesktopSmall: [414, 3]

                });

            });
        </script>
        <style type="text/css">
            .slides li {
                list-style:none;
            }
            #userInfo {
                width:100%;
                height:100px;
                background-color:aquamarine;
            }
